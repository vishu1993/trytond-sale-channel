        expr="/form/notebook/page[@id=&quot;preferences&quot;]/field[@name=&quot;language&quot;]"
        position="after">
        <label name="current_channel"/>
        <field name="current_channel" widget="selection"/>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id=&quot;preferences&quot;]"
        position="after">
        <page string="Channels" col="2" id="channels">
